/* rajdhani-300 - latin */
@font-face {
  font-family: 'Rajdhani';
  font-style: normal;
  font-weight: 300;
  src: url($text-font-path + '/rajdhani/rajdhani-v7-latin-300.eot'); /* IE9 Compat Modes */
  src: local('Rajdhani Light'), local('Rajdhani-Light'),
    url($text-font-path + '/rajdhani/rajdhani-v7-latin-300.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
    url($text-font-path + '/rajdhani/rajdhani-v7-latin-300.woff2') format('woff2'), /* Super Modern Browsers */
    url($text-font-path + '/rajdhani/rajdhani-v7-latin-300.woff') format('woff'), /* Modern Browsers */
    url($text-font-path + '/rajdhani/rajdhani-v7-latin-300.ttf') format('truetype'), /* Safari, Android, iOS */
    url($text-font-path + '/rajdhani/rajdhani-v7-latin-300.svg#Rajdhani') format('svg'); /* Legacy iOS */
}

