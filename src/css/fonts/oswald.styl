/* oswald-300 - cyrillic_latin */
@font-face {
  font-family: 'Oswald';
  font-style: normal;
  font-weight: 300;
  src: url($text-font-path + '/oswald/oswald-v16-cyrillic_latin-300.eot'); /* IE9 Compat Modes */
  src: local('Oswald Light'), local('Oswald-Light'),
    url($text-font-path + '/oswald/oswald-v16-cyrillic_latin-300.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
    url($text-font-path + '/oswald/oswald-v16-cyrillic_latin-300.woff2') format('woff2'), /* Super Modern Browsers */
    url($text-font-path + '/oswald/oswald-v16-cyrillic_latin-300.woff') format('woff'), /* Modern Browsers */
    url($text-font-path + '/oswald/oswald-v16-cyrillic_latin-300.ttf') format('truetype'), /* Safari, Android, iOS */
    url($text-font-path + '/oswald/oswald-v16-cyrillic_latin-300.svg#Oswald') format('svg'); /* Legacy iOS */
}

